<%= form_with url: login_path, method: :post do |f| %>
	<div class='col-md-6'>
  	<%= f.label :email %>
  </div>
  <div class='col-md-6'>
  	<%= f.email_field :email %>
	</div><br>
	<div class='col-md-6'>
  	<%= f.label :password %>
  </div>
  <div class='col-md-6'>
  	<%= f.password_field :password %>
  </div><br>
  <%= f.submit 'Sign in' %>
<% end %>